{
  "name": "@port/database",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "db:generate": "dotenv -e ../../.env.development -- prisma generate",
    "db:migrate": "dotenv -e ../../.env.development -- prisma migrate dev",
    "db:push": "dotenv -e ../../.env.development -- prisma db push",
    "db:studio": "dotenv -e ../../.env.development -- prisma studio",
    "db:seed": "dotenv -e ../../.env.development -- ts-node prisma/seed.ts",
    "db:generate:prod": "dotenv -e ../../.env -- prisma generate",
    "db:push:prod": "dotenv -e ../../.env -- prisma db push",
    "db:seed:prod": "dotenv -e ../../.env -- ts-node prisma/seed.ts"
  },
  "prisma": {
    "seed": "dotenv -e ../../.env.development -- ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "dotenv-cli": "^8.0.0",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
